module Factorix
  # Represents the state of a MOD in a MOD list
  class MODState < Data
    attr_reader enabled: bool

    attr_reader version: MODVersion?

    def initialize: (enabled: bool, ?version: MODVersion?) -> void

    def self.[]: (enabled: bool, ?version: MODVersion?) -> MODState

    def enabled?: () -> bool
  end
end
